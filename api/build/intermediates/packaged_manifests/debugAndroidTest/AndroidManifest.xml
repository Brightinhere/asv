<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.battlelancer.seriesguide.api.test" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="31" />

    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:functionalTest="false"
        android:handleProfiling="false"
        android:label="Tests for com.battlelancer.seriesguide.api.test"
        android:targetPackage="com.battlelancer.seriesguide.api.test" />
    <!-- JobIntentService manages wake lock on its own pre-O -->
    <uses-permission
        android:name="android.permission.WAKE_LOCK"
        android:maxSdkVersion="25" />

    <application
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true" >
        <uses-library android:name="android.test.runner" />
    </application>

</manifest>